{
  "name": "plaisio/form",
  "description": "PhpPlaisio: Form",
  "keywords": [
    "PhpPlaisio",
    "Form"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-mbstring": "*",
    "ext-tidy": "*",
    "plaisio-ts/helper-cast": "^2.1.0",
    "plaisio-ts/kernel": "^2.1.0",
    "plaisio/exception": "^1.3.0",
    "plaisio/helper-html": "^4.2.0",
    "plaisio/helper-url": "^1.3.1",
    "plaisio/kernel": "^3.3.1",
    "plaisio/obfuscator": "^2.2.0",
    "plaisio/request": "^2.0.0",
    "plaisio/session": "^5.1.0",
    "setbased/exception": "^2.6.0",
    "setbased/helper-cast": "^3.1.0"
  },
  "require-dev": {
    "ext-dom": "*",
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.25",
    "plaisio/console-assets": "^1.3.0",
    "plaisio/console-kernel": "^1.2.0",
    "plaisio/console-type-script": "^1.2.0",
    "plaisio/request-core": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Form\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Form\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
