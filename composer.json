{
  "name": "plaisio/form",
  "description": "PhpPlaisio: Form",
  "keywords": [
    "PhpPlaisio",
    "Form"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "ext-mbstring": "*",
    "plaisio/exception": "^1.0",
    "plaisio/helper-html": "^1.0",
    "plaisio/helper-url": "^1.0",
    "plaisio/kernel": "^1.0",
    "plaisio/obfuscator": "^1.0",
    "setbased/exception": "^2.1",
    "setbased/helper-cast": "^2.1"
  },
  "require-dev": {
    "ext-dom": "*",
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0",
    "plaisio/request-core": "^0.10.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Form\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Form\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
