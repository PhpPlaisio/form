{
  "name": "plaisio/form",
  "description": "PhpPlaisio: Form",
  "keywords": [
    "PhpPlaisio",
    "Form"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "ext-mbstring": "*",
    "ext-tidy": "*",
    "plaisio-ts/helper-cast": "^2.0.0",
    "plaisio-ts/kernel": "^1.1.0",
    "plaisio/exception": "^1.1.0",
    "plaisio/helper-html": "^4.0.0",
    "plaisio/helper-url": "^1.1.2",
    "plaisio/kernel": "^3.1.1",
    "plaisio/obfuscator": "^2.0.0",
    "plaisio/request": "^0.13.0",
    "plaisio/session": "^4.0.0",
    "setbased/exception": "^2.2.0",
    "setbased/helper-cast": "^2.1.1"
  },
  "require-dev": {
    "ext-dom": "*",
    "phing/phing": "^2.17.0",
    "phpunit/phpunit": "^9.5.11",
    "plaisio/console": "^2.5.0",
    "plaisio/request-core": "^0.13.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Form\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Form\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
