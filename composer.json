{
  "name": "setbased/abc-form",
  "description": "ABC-Framework: Form",
  "keywords": [
    "ABC-Framework",
    "Form"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "ext-mbstring": "*",
    "ext-tidy": "*",
    "plaisio/exception": "^3.0",
    "plaisio/html": "^3.0",
    "plaisio/obfuscator": "^3.0",
    "plaisio/url": "^3.0",
    "setbased/abc": "^0.10.0",
    "setbased/exception": "^2.0",
    "setbased/helper-cast": "^2.0"
  },
  "require-dev": {
    "ext-dom": "*",
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^7.0.0 || ^8.0.0",
    "setbased/abc-request-core": "^0.9.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Abc\\Form\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Abc\\Form\\Test\\": "test/"
    }
