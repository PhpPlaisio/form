{
  "name": "plaisio/form",
  "description": "PhpPlaisio: Form",
  "keywords": [
    "PhpPlaisio",
    "Form"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-mbstring": "*",
    "ext-tidy": "*",
    "plaisio-ts/helper-cast": "^2.1.0",
    "plaisio-ts/kernel": "^2.1.0",
    "plaisio/exception": "^1.2.0",
    "plaisio/helper-html": "^4.1.0",
    "plaisio/helper-url": "^1.2.0",
    "plaisio/kernel": "^3.2.1",
    "plaisio/obfuscator": "^2.1.0",
    "plaisio/request": "^0.13.1",
    "plaisio/session": "^5.0.0",
    "setbased/exception": "^2.3.0",
    "setbased/helper-cast": "^3.0.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "ext-dom": "*",
    "phing/phing": "^3.0.0-RC4",
    "phpunit/phpunit": "^9.6.3",
    "plaisio/console-kernel": "^1.0.1",
    "plaisio/console-type-script": "^1.0.0",
    "plaisio/request-core": "^0.13.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Form\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Form\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
